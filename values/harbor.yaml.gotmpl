expose:
  type: ingress
  ingress:
    hosts:
      core: harbor.{{ .Values.host.internal }}
    annotations:
      ingress.kubernetes.io/ssl-redirect: "true"
      cert-manager.io/cluster-issuer: local-ca-issuer
  tls:
    enabled: true
    certSource: secret
    secret:
      secretName: harbor-tls

trivy:
  enabled: false

notary:
  enabled: false

chartmuseum:
  enabled: false

externalURL: https://harbor.{{ .Values.host.internal }}
secretKey: {{ .Values.harbor.secret }}
